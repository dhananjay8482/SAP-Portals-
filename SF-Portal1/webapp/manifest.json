{
	"_version": "1.7.0",
	"sap.app": {
		"id": "SF-Portal1",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZDS_SF_SRV": {
				"uri": "/sap/opu/odata/sap/ZDS_SF_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			},
			"ZDS_MP_SRV": {
				"uri": "/sap/opu/odata/sap/ZDS_MP_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/ZDS_MP_SRV/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "SF-Portal1.view.ViewFrom",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "SF-Portal1.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"targets": {
				"TargetViewFrom": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": "true",
					"viewName": "ViewFrom",
					"viewLevel": 1
				},
				"TargetViewTo": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "ViewTo",
					"viewLevel": 2
				},
				"TargetViewToParameter": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "ViewTo",
					"viewLevel": 2
				},
				"TargetViewRes": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "ViewRes",
					"viewLevel": 3
				},
				"TargetViewResParameter": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "ViewRes",
					"viewLevel": 3
				},
				"TargetPlannedOrder": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "PlannedOrder"
				},
				"TargetProductionOrder": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "ProductionOrder"
				}
			},
			"routes": [{
				"name": "TargetViewFrom",
				"pattern": "RouteViewFrom",
				"titleTarget": "",
				"greedy": false,
				"target": ["TargetViewFrom"]
			}, {
				"name": "TargetViewTo",
				"pattern": "RouteViewTo",
				"titleTarget": "",
				"greedy": false,
				"target": ["TargetViewTo"]
			}, {
				"name": "TargetViewToParameter",
				"pattern": "RouteViewTo/{parameter}",
				"titleTarget": "",
				"greedy": false,
				"target": ["TargetViewToParameter"]
			}, {
				"name": "TargetViewRes",
				"pattern": "RouteViewRes",
				"titleTarget": "",
				"greedy": false,
				"target": ["TargetViewRes"]
			}, {
				"name": "TargetViewResParameter",
				"pattern": "RouteViewRes/{parameter}",
				"titleTarget": "",
				"greedy": false,
				"target": ["TargetViewResParameter"]
			}, {
				"name": "TargetProductionOrder",
				"pattern": "RouteProductionOrder",
				"titleTarget": "",
				"greedy": false,
				"target": ["TargetProductionOrder"]
			}, {
				"name": "TargetPlannedOrder",
				"pattern": "RoutePlannedOrder",
				"titleTarget": "",
				"greedy": false,
				"target": ["TargetPlannedOrder"]
			}],
			"config": {
				"viewPath": "SF-Portal1.view",
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false,
				"bypassed": {
					"target": []
				}
			}
		}
	}
}